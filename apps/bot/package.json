{
	"name": "shinigawa-chan-bot",
	"version": "0.0.1",
	"description": "A multi-feature Discord bot that is designed to assist you in your daily tasks.",
	"author": "@NotKaskus",
	"license": "MIT",
	"type": "module",
	"main": "./dist/Ciel.js",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/NotKaskus/ciel.git",
		"directory": "apps/bot/discord"
	},
	"homepage": "https://github.com/NotKaskus/ciel/tree/main/apps/bot/discord",
	"engines": {
		"node": ">=20.x"
	},
	"scripts": {
		"build": "tsup",
		"check": "tsc",
		"dev": "tsup --watch --onSuccess \"bun run start\"",
		"start": "node --preserve-symlinks --enable-source-maps ./dist/Ciel.js",
		"lint": "eslint --ext ts src tests && bun run check",
		"lint:fix": "eslint --fix --ext js,ts --ext ts src tests",
		"format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\" && prisma format",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:sync": "bun run db:generate && bun run db:push",
		"db:deploy": "prisma migrate deploy",
		"db:migrate": "prisma migrate reset && prisma migrate dev"
	},
	"dependencies": {
		"@kbotdev/plugin-modules": "^3.0.0",
		"@killbasa/redis-utils": "^0.0.10",
		"@prisma/client": "^5.15.1",
		"@sapphire/decorators": "^6.1.0",
		"@sapphire/discord.js-utilities": "^7.3.0",
		"@sapphire/duration": "^1.1.2",
		"@sapphire/fetch": "^3.0.2",
		"@sapphire/framework": "^5.2.1",
		"@sapphire/plugin-api": "^6.1.1",
		"@sapphire/plugin-logger": "^4.0.2",
		"@sapphire/plugin-scheduled-tasks": "^10.0.1",
		"@sapphire/utilities": "^3.16.2",
		"@sentry/integrations": "^7.114.0",
		"@sentry/node": "^8.8.0",
		"@skyra/gifenc": "^1.0.1",
		"bufferutil": "^4.0.8",
		"canvas": "^2.11.2",
		"canvas-constructor": "^7.0.2",
		"colorette": "^2.0.20",
		"discord-api-types": "^0.37.87",
		"discord.js": "^14.15.3",
		"dotenv": "^16.4.5",
		"fuzzysort": "^3.0.1",
		"humanize-duration": "^3.32.1",
		"ioredis": "^5.4.1",
		"prom-client": "^15.1.2",
		"utf-8-validate": "^6.0.4",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@sentry/types": "^7.93.0",
		"@types/humanize-duration": "^3.27.4",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"eslint": "8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.1",
		"prisma": "^5.15.1",
		"tsup": "^8.2.2",
		"typescript": "^5.3.3"
	},
	"imports": {
		"#config": "./dist/config.js",
		"#lib/*": "./dist/lib/*.js",
		"#modules/*": "./dist/modules/*.js",
		"#plugins/*": "./dist/plugins/*.js"
	}
}
